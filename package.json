  "homepage": "https://github.com/doronnahum/feathers-mongoose-casl.git",
  "main": "lib/",
  "keywords": [
    "feathers",
    "casl",
    "mongoosejs"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
